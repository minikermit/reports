<h1>New scenario</h1>

<% form_for(@scenario) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :Code %><br />
    <%= f.text_field :Category_Code %>
  </p>
  <p>
    <%= f.label :Name %><br />
    <%= f.text_field :Category_Name %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', scenarios_path %>

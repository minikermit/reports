<h1>Listing product_categories</h1>

<table>
  <tr>
  </tr>

<% for product_category in @product_categories %>
  <tr>
    <td><%= link_to 'Show', product_category %></td>
    <td><%= link_to 'Edit', edit_product_category_path(product_category) %></td>
    <td><%= link_to 'Destroy', product_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product_category', new_product_category_path %>
